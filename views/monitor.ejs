                            <span class="hamburger-inner"></span>
                        </span>
                    </div>
            </div>
	    <div class="hdr_title_sec">
			{{side_menu_slcted_name_func(side_menu_slcted_flg)}}
	    </div>
	    <div class="pull-right">
		<div class="dropdown pull-left">
            	<div class="header-icon waves-effect pull-left dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-building" aria-hidden="true"></i><span class="text-dark" style="text-transform: initial;">{{slcted_indus_dic['project_name']}}</span></div>
            	<div class="dropdown-menu dropdown-menu-tas">
			<div class="md-form md-form-tas" style="margin: 5px 23px;" ng-if="0">
                        	<input class="form-control" type="text" placeholder="Search" aria-label="Search" ng-model="filterIndusVal" ng-keyup="enter($event)" style="font-size: 14px;" autofocus>
			 </div>
			<div class="dropdown-item-tas-full">
	                	<div class="dropdown-item-tas waves-effect" ng-repeat="ind in industry_type_list" ng-click="slct_indus_func(ind, 0)" ng-class="{'drp_active': slcted_indus_dic['project_id'] == ind['project_id']}" title="{{ind['project_name']}}">{{ind['project_name']}}</div>
			</div>
            	</div>
	    </div>
	    <div class="dropdown pull-left">
	    <div class="header-icon waves-effect pull-left dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-file" aria-hidden="true"></i><span class="text-dark">{{slcted_comp_dic['company_name']}}</span></div>
	    <div class="dropdown-menu dropdown-menu-tas">
		<div class="md-form md-form-tas" style="margin: 5px 23px;">
			<input class="form-control" type="text" placeholder="Search" aria-label="Search" ng-model="filterRow_comp" ng-keyup="enter($event)" ng-change="filterRowFunc()" style="font-size: 14px;" autofocus>
		</div>
		<div class="dropdown-item-tas-full">
			<div class="dropdown-item-tas waves-effect" ng-repeat="comp in comp_data | filter: filterRow_comp" ng-click="comp_click_func(comp)" ng-class="{'drp_active': slcted_comp_dic['company_id'] == comp['company_id']}" title="{{comp['company_name']}}">{{comp['company_name']}}</div>
		</div>
	    </div>
            </div>
		    <div class="header-icon waves-effect pull-left" ng-click="show_alert_func()">
			<i class="fa fa-bell" aria-hidden="true" ng-class="{'icon-anim-pulse': show_alert_flg}"></i>
