					<div class="comp_grp_div" ng-repeat="cn in comp_data | filter:filterComCmpyRow track by $index">
						<div class="comp_grp_lft_sec">
							<div class="comp_grp_lft_sec_logo">
							    <img src="src/images/comp_dummy.png" width=""> 
