</div>
<div class="ts_box" ng-class="{'closed-sidebar': !side_menu_active}">
	<tas-upload config="upload_scope" ng-show="side_menu_slcted_flg['k'] == 'crawling_monitoring'"></tas-upload>
	<tas-source  config="source_scope" ng-show="side_menu_slcted_flg['k'] == 'crawling_monitoring'"></tas-source>

	<div class="ts_box_menu app-sidebar">
		<div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu metismenu">
                            <li class="app-sidebar__heading">Project Builder Menu</li>
			    <li class="mm-active" ng-repeat="menu in side_menu_list" ng-if="menu_sec_show_func(menu)">
                                <a href="#" aria-expanded="true" ng-click="side_menu_click_func(menu, menu['k'])" ng-class="{'active': side_menu_slcted_flg['k'] == menu['k'] || (side_mmenu_k == menu['k'] && !side_menu_active), 'mtr_disable_btn': menu_showing[menu['k']] == 'N'}" title="{{menu['n']}}" key="{{menu['k']}}">
				    <i class="fa {{menu['fa_icon']}} metismenu-icon" aria-hidden="true"></i>
                                    {{menu['n']}}
                                </a>
				<ul class="mm-collapse mm-show" style="" ng-if="menu['submenu'].length">
                                    <!--li ng-repeat="smenu in menu['submenu']" ng-if="smenu['k'] && (menu_showing[smenu['k']] == 'Y' || (menu_showing[smenu['k']] == 'N' && smenu['submenu'].length))"-->
                                    <li ng-repeat="smenu in menu['submenu']" ng-if="menu_sec_show_func(smenu)">
                                        <a href="#" ng-click="side_menu_click_func(smenu, menu['k'])" ng-class="{'mm-active': side_menu_slcted_flg['k'] == smenu['k'], 'mtr_disable_btn': menu_showing[smenu['k']] == 'N'}" title="{{smenu['n']}}" key="{{smenu['k']}}">
                                            <i class="metismenu-icon">
                                            </i>{{smenu['n']}}
                                        </a>
					<ul class="mm-collapse mm-show" style="" ng-if="smenu['submenu'].length">
					    <li ng-repeat="ssmenu in smenu['submenu']" ng-if="ssmenu['k'] && (menu_showing[ssmenu['k']] == 'Y')">
						<a href="#" ng-click="side_menu_click_func(ssmenu, menu['k'])" ng-class="{'mm-active': side_menu_slcted_flg['k'] == ssmenu['k'], 'mtr_disable_btn': false}" title="{{ssmenu['n']}}" key="{{ssmenu['k']}}">
						    <i class="metismenu-icon">
						    </i>{{ssmenu['n']}}
						</a>

					    </li>
					</ul>
                                    </li>
				</ul>
                            </li>
                        </ul>
		</div>
	</div>

	<div class="noti_pop" ng-if="show_alert_pop_flg">
		<div class="noti_pop_bg" ng-click="cls_show_alert_func()"></div>
		<div class="noti_pop_cnt" ng-class="{'open': show_alert_pop_flg}">
			<div class="no_noti_txt" ng-if="!doc_updated_list.length">No new notifications</div>
			<div class="noti_pop_cnt_fl"  ng-if="doc_updated_list.length">
	                        <div class="no_noti_txt" style="font-weight: normal;">Notifications</div>
				<div class="no_noti_p">
					<div class="no_noti_pt" ng-repeat="doc in doc_updated_list" ng-click="status_chck_change_func(doc)">
                                                <span class="no_noti_crc" ng-class="{noti_read: doc['read']}"></span>
                                                <div class="no_noti_pt_p">{{doc['indus']['project_name']}}</div>
                                                <div class="no_noti_pt_c">{{doc['comp']['company_name']}}</div>
                                                <div class="no_noti_pt_tx">Doc id <span class="no_noti_pt_t_doc badge badge-warning">{{doc['doc_id']}}</span> updated <span ng-if="doc['status']=='E'" style="font-weight: bold;color: red;margin-left: 20px;">(ERROR)</span> <span ng-if="doc['status']=='Y'" style="font-weight: bold;color: #4CAF50; margin-left: 20px;">(SUCCESS)</span></div>
                                        </div>
				</div>
			</div>
		</div>
	</div>


<div class="mtr_main" ng-class="{'closed-sidebar': !side_menu_active}">
	<div ng-if="side_menu_slcted_flg['k'] == 'pre_taxo'" class="mtr_main__sb_sec">
		<iframe id="pre_taxo_id" src=""></iframe>
	</div>
	<div ng-if="side_menu_slcted_flg['k'] == 'ocr'" class="mtr_main__sb_sec">
                <iframe id="slt_frame_id" src=""></iframe>
        </div>
	<div ng-if="side_menu_slcted_flg['k'] == 'taxonomy_link' || side_menu_slcted_flg['k'] == 'search'  || side_menu_slcted_flg['k'] == 'focused_extrc' || side_menu_slcted_flg['k'] == 'model_mapping_rca' || side_menu_slcted_flg['k'] == 'edit_op_temp'" class="mtr_main__sb_sec">
                <iframe id="slt_frame_id" src=""></iframe>
        </div>
	<div class="mtr_main__sb_sec mtr_card" ng-if="side_menu_slcted_flg['k'] == 'data_models'">
		<div class="mtr_card__header">
			<div class="mtr_card__title">{{side_menu_slcted_name_func(side_menu_slcted_flg)}}</div>
		</div>
		<div class="mtr_card__body" style="height: calc(100% - 35px);">
			<enterprise-data config="enterprise"></enterprise-data>
		</div>
	</div>
	<div class="mtr_main__sb_sec" ng-if="side_menu_slcted_flg['k'] == 'data_builder_rca' || side_menu_slcted_flg['k'] == 'review_output'">
		<builder-rca config="buider_rca" class="module_div"></builder-rca>	
	</div>
	<div ng-if="doc_view_enable" class="mtr_main__sb_sec">
		<div class="mtr_main__sb_sec_tabs">
			<div class="rca_divide">
				<div class="mtr_main__sb_sec_doc">Doc Id: <b>{{slcetd_doc_view_dic['d']}}</b></div>	
				<div class="mtr_main__sb_sec_doc rca_adjust">Doc Name: <b>{{slcetd_doc_view_dic['doc_name']}}</b></div>	
			</div>
			<div class="rca_divide_docs">
				<div class="dropdown pull-right" style="height: 30px;">
					<div class="header-icon waves-effect pull-left dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="height:34px;line-height:35px;">
						<div class="header_icon_txt_hdr">
							<i class="fa fa-building" aria-hidden="true"></i><span class="text-dark" style="text-transform: initial;" title="{{slcted_rca_dic['t']['d']+' - '+slcted_rca_dic['t']['doc_name']}}">{{slcted_rca_dic['t']['d']+' - '+slcted_rca_dic['t']['doc_name']}}</span>
						</div>
					</div>
					<div class="dropdown-menu dropdown-menu-tas" style="width: 100%;">
						<div class="dropdown-item-tas-full">
							<div class="dropdown-item-tas waves-effect" ng-repeat="doc in gridOptionsDoc['data']" ng-click="change_rca_docs(doc)" title="{{doc['d']+' - '+doc['doc_name']}}">{{doc['d']+' - '+doc['doc_name']}}</div>
						</div>
					</div>
				</div>
			</div>	
                </div>
                <div class="" ng-if="side_menu_slcted_flg['k'] == 'slt'" style="height: calc(100% - 30px);float: left;width: 100%;">
                     <textapplicator config="modeltraining" class="apt"></textapplicator>
                </div>
		<iframe id="slt_frame_id" src=""  ng-if="(side_menu_slcted_flg['k'] == 'output' && slcted_indus_dic['project_id'] == '60')"  class="mtr_main__sb_sec_frame"></iframe> 
		<ref-div config="refOptions" ng-if="side_menu_slcted_flg['k'] == 'toc'" class="mtr_main__sb_sec_frame"> </ref-div> 
		<div ng-if="side_menu_slcted_flg['k'] == 'exha_data' || side_menu_slcted_flg['k'] == 'text_to_table' || side_menu_slcted_flg['k'] ==  'all_tables' || side_menu_slcted_flg['k'] ==  'foot_notes' || side_menu_slcted_flg['k'] ==  'sen_table_desc' || side_menu_slcted_flg['k'] ==  'sen_addit' || side_menu_slcted_flg['k'] ==  'other_txt_t_tble' || side_menu_slcted_flg['k'] == 'table_analys'" class="exha_data_contents">
                      <informations-units config="information_scope"></informations-units>
                </div>
		<div ng-if="side_menu_slcted_flg['k'] == 'output' && slcted_indus_dic['project_id'] != 'FE' && slcted_indus_dic['project_id'] != '60'" class="mtr_main__sb_sec_frame">
			<!--tas-output-view config="outputviewOptions" class="module_div"></tas-output-view-->
			<runapplicator config="modeltraining_1" class="apt"></runapplicator>
		</div>
	</div>
	
	<div ng-if="side_menu_slcted_flg['k'] == 'output' && slcted_indus_dic['project_id'] == 'FE'" class="mtr_main__sb_sec">
		 <div class="mtr_main__sb_sec_tabs">
			<div class="mtr_main__sb_sec_tab waves-effect" ng-class="{'active': output_sclted_sub_menu == 'op_view'}" ng-click="output_sclted_sub_click_func('op_view')">Comparative View</div>
			<div class="mtr_main__sb_sec_tab waves-effect" ng-class="{'active': output_sclted_sub_menu == 'analyt'}" ng-click="output_sclted_sub_click_func('analyt')">Analytics</div>
		 </div>
		 <iframe id="slt_frame_id" src="" class="mtr_main__sb_sec_frame"></iframe>
		 <!--div ng-if="output_sclted_sub_menu == 'op_view'">
			 <iframe id="slt_frame_id" src="" class="mtr_main__sb_sec_frame"></iframe>
		</div>
		<div ng-if="output_sclted_sub_menu == 'analyt'">
			<validated-data config="validated_data" class="vali_body"></validated-data>
		</div-->
	</div>
	<div ng-if="!doc_view_enable && side_menu_slcted_flg['doc_view'] && (side_menu_slcted_flg['k'] != 'output' ||  slcted_indus_dic['project_id'] != 'FE') && side_menu_slcted_flg['k'] != 'crawling_monitoring'" class="mtr_main__sb_sec mtr_card">
		<div class="mtr_card__header">
                	<div class="mtr_card__title">Documents</div>
			<div class="dropdown pull-right" style="width: auto;">
                        	<div class="waves-effect main_tab_btn waves-effect pull-left main_tab_sub_btn" ng-click="show_comp_view()" data-toggle="tooltip" title="" style="line-height: 35px;height: 35px;font-size: 14px;" ng-class="{active: cmp_comp_flg}" data-original-title="Compare"><i class="fa fa-balance-scale" aria-hidden="true"></i>
                        	</div>
                	</div>
			<span class="pull-right sch_cls" ng-click="review_across()" title="Comparative View" ng-if="side_menu_slcted_flg['k'] == 'output' && slcted_indus_dic['project_id'] != '60'" style="line-height: 35px;width: 40px;text-align: center;">
				<i class="fa fa-arrow-right" style="font-size: 14px;color: #000;"></i>
			</span>
		</div>
